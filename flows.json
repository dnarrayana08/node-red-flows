        "id": "e0a513cf02baa525",
        "type": "function",
        "z": "509d1b9b92a63e42",
        "name": "process sensor data",
        "func": "let data = msg.payload;\n\n// Ensure numbers\nmsg.temperature = Number(data.temperature);\nmsg.humidity = Number(data.humidity);\n\n// Status text (explicit comparison)\nmsg.acStatus = (data.ac === \"ON\") ? \"ON\" : \"OFF\";\nmsg.lightStatus = (data.light === \"ON\") ? \"ON\" : \"OFF\";\n\n// Timestamp\nmsg.timestamp = new Date().toLocaleTimeString();\n\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
