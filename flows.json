    {
        "id": "5d4173ad71349529",
        "type": "function",
        "z": "509d1b9b92a63e42",
        "name": "function 1",
        "func": "let data = msg.payload;\n\nlet line = `Temp=${data.temperature}°C, Humidity=${data.humidity}%, LDR=${data.ldr}, AC=${data.ac}, Light=${data.light}, Time=${data.timestamp}\\n`;\n\n// Get previous log or start empty\nlet log = flow.get('sensorLog') || '';\nlog += line;\n\n// Limit log length (optional)\nif (log.length > 1000) {\n  log = log.slice(log.length - 1000);\n}\n\nflow.set('sensorLog', log);\nmsg.payload = log;\nreturn msg;\n",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
